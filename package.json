{
  "name": "autoupd-example",
  "version": "1.0.0",
  "description": "",
  "main": "src/main/index.js",
  "scripts": {
    "start": "ELECTRON_IS_DEV=1 electron .",
    "dev": "ELECTRON_IS_DEV=1 electron-webpack dev",
    "clean": "rm -rf dist",
    "compile": "yarn clean && electron-webpack",
    "dist:linux": "yarn compile && electron-builder --linux",
    "dist": "yarn compile && electron-builder --win --linux",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null",
    "dist:dir32": "yarn dist:dir --ia32"
  },
  "author": "Rafael Pereira",
  "license": "ISC",
  "dependencies": {
    "electron-is-dev": "^1.1.0",
    "source-map-support": "^0.5.12"
  },
  "devDependencies": {
    "electron": "4.2.11",
    "electron-builder": "^21.0.11",
    "electron-webpack": "^2.7.4",
    "webpack": "~4.35.3"
  },
  "build": {
    "appId": "autoupd-example",
    "compression": "store",
    "asar": true,
    "asarUnpack" :["./static"],
    "win":{
      "target": "nsis",
      "icon": "static/img/tux.ico"
    }
  }
}
